include(default)
target_host=arm-none-eabi
arm_gnu_toolchain=/opt/gcc-arm-none-eabi-9-2020-q2-update

[settings]
arch=armv8
build_type=Release
compiler=clang
compiler.libcxx=libc++
compiler.version=9

[build_requires]

[options]

[env]
PATH=[$arm_gnu_toolchain/bin]
CHOST=$target_host
AR=$target_host-ar
AS=$target_host-as
RANLIB=$target_host-ranlib
CC=$target_host-gcc
CXX=$target_host-g++
LD=$target_host-ld
STRIP=$target_host-strip

